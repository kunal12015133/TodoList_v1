<%- include("header") -%> 
    <div class="box" id="heading">
        <h1> <%= listTitle %> </h1>
    </div>
    <div class="box">
        <% for (let i = 0; i < newItem.length; i++) { %>
        <div class="item">
            <input type="checkbox">
            <p> <%= newItem[i]  %> </p>
        </div>
        
        <% } %>

        <form class="item" action= <%= route %> method="post">
            <input type="text" name="newItem" autocomplete="off" placeholder="New Item">
            <button type="submit" name="button" value=<%= route %> >+</button>
        </form>
    </div>


    <%- include("footer") -%>   
